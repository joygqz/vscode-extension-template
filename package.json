{
  "publisher": "joygqz",
  "name": "vscode-extension-template",
  "displayName": "Extension Name",
  "version": "0.0.0",
  "description": "这是一个测试扩展。",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/joygqz/vscode-extension-template.git"
  },
  "keywords": [],
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "icon": "./images/logo.png",
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "dist/*"
  ],
  "engines": {
    "vscode": "^1.104.0"
  },
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "vscode-extension-template.helloWorld",
        "title": "Hello World",
        "category": "Extension Name",
        "icon": {
          "light": "images/logo.svg",
          "dark": "images/logo.svg"
        }
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Extension Name",
      "properties": {
        "vscode-extension-template.test": {
          "type": "boolean",
          "default": true,
          "description": "Enable or disable the feature."
        }
      }
    },
    "menus": {}
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "package": "pnpm run check-types && pnpm run lint && node esbuild.js --production",
    "compile": "pnpm run check-types && pnpm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "check-types": "tsc --noEmit",
    "lint": "eslint .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "ext:package": "vsce package",
    "ext:publish": "vsce publish",
    "release": "bumpp -x \"pnpm run changelog\" -a"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "~22.18.8",
    "@types/vscode": "^1.104.0",
    "@vscode/vsce": "^3.6.2",
    "bumpp": "^10.2.3",
    "conventional-changelog-cli": "^5.0.0",
    "esbuild": "^0.25.10",
    "eslint": "^9.36.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3"
  }
}
